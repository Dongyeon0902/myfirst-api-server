apiVersion: v1
kind: ConfigMap
metadata:
  name: jenkins-config
  namespace: jenkins
data:
  jenkins.yaml: |
    jenkins:
      systemMessage: "Jenkins with Kaniko Integration - Harbor Registry + EKS Cluster"
      numExecutors: 2
      mode: NORMAL
      
      securityRealm:
        local:
          allowsSignup: false
          users:
          - id: "admin"
            password: "admin"
            
      authorizationStrategy:
        globalMatrix:
          permissions:
          - "Overall/Administer:admin"
          - "Overall/Read:authenticated"
          
    clouds:
    - kubernetes:
        name: "kubernetes"
        serverUrl: "https://kubernetes.default:443"
        serverCertificate: ""
        skipTlsVerify: true
        namespace: "jenkins"
        jenkinsUrl: "http://jenkins:8080"
        jenkinsTunnel: "jenkins:50000"
        connectTimeout: 5
        readTimeout: 15
        retentionTimeout: 5
        maxRequestsPerHost: 32
        
    credentials:
      system:
        domainCredentials:
        - credentials:
          - string:
              scope: GLOBAL
              id: "docker-registry-url"
              description: "Harbor Registry URL"
              secret: "amdp-registry.skala-ai.com/skala25a"
          - usernamePassword:
              scope: GLOBAL
              id: "docker-registry-credentials"
              description: "Harbor Registry Credentials"
              username: "robot$skala25a"
              password: "1qB9cyusbNComZPHAdjNIFWinf52xaBJ"
          - file:
              scope: GLOBAL
              id: "kubeconfig-file"
              description: "EKS Cluster Kubeconfig"
              fileName: "kubeconfig"
              secretBytes: ""
              
    unclassified:
      location:
        url: "https://jenkins.skala25a.project.skala-ai.com/"  # 실제 도메인으로 변경
        adminAddress: "yongwooyi@skala-ai.com"
